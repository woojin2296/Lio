{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends":["airbnb","prettier/react", "eslint:recommended","plugin:prettier/recommended"],
  // prettier/react 추가
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "react/jsx-filename-extension": [
      "error", {
        "extensions": [".js", ".jsx"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}